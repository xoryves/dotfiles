<?xml version=”1.0" encoding=”UTF-8"?>
<!DOCTYPE plist PUBLIC “-//Apple Computer//DTD PLIST 1.0//EN” “http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version=”1.0">
    <dict>
        <key>Label</key>
        <string>ves.caddyserver</string>
        <key>ProgramArguments</key>
        <array>
	        <string>/Users/_vaultwarden/caddy</string>
	        <string>run</string>
        </array>
        <key>EnvironmentVariables</key>
        <dict>
            <key>HOME</key>
            <string>/Users/_vaultwarden</string>
        </dict>
        <key>StandardOutPath</key>
        <string>/Users/_vaultwarden/caddy.log</string>
        <key>StandardErrorPath</key>
        <string>/Users/_vaultwarden/caddy_error.log</string>
        <key>WorkingDirectory</key>
        <string>/Users/_vaultwarden</string>
        <key>SoftResourceLimits</key>
        <dict>
	        <key>NumberOfFiles</key>
	        <integer>8192</integer>
        </dict>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>UserName</key>
        <string>_vaultwarden</string>
        <key>GroupName</key>
        <string>_vaultwarden</string>
        <key>ThrottleInterval</key>
        <integer>30</integer>
    </dict>
</plist>